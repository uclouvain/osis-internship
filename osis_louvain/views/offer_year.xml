<?xml version="1.0" encoding="UTF-8"?>
<openerp>
  <data>
    <!-- Tree -->
    <record id="offer_year_tree_view" model="ir.ui.view">
      <field name="name">offer_year.tree.view</field>
      <field name="model">osis.offer_year</field>
      <field eval="16" name="priority"/>
      <field name="arch" type="xml">
        <tree>
          <field name="offer_id"/>
          <field name="structure_id"/>
          <field name="academic_year_id"/>
        </tree>
      </field>

    </record>
    <!-- Form -->
    <record id="offer_year_form" model="ir.ui.view">
      <field name="name">osis.view.offer_year.form</field>
      <field name="model">osis.offer_year</field>

      <field name="arch" type="xml">
        <form string="Offer Year Form">
          <group col="4">
            <field name="offer_id"/>
            <field name="structure_id"/>
            <field name="academic_year_id"/>
          </group>
        </form>
      </field>
    </record>

  </data>
</openerp>
